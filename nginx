server {
	server_name mia-chatbot.ru www.mia-chatbot.ru;
	charset off;
	index index.php index.html;
	disable_symlinks if_not_owner from=$root_path;
	include /etc/nginx/vhosts-includes/*.conf;
	include /etc/nginx/vhosts-resources/mia-chatbot.ru/*.conf;
	access_log /var/www/httpd-logs/mia-chatbot.ru.access.log;
	error_log /var/www/httpd-logs/mia-chatbot.ru.error.log notice;
	ssi on;
	set $root_path /var/www/www-root/data/www/mia-chatbot.ru;
	root $root_path;
	gzip on;
	gzip_comp_level 5;
	gzip_disable "msie6";
	gzip_types text/plain text/css application/json application/x-javascript text/xml application/xml application/xml+rss text/javascript application/javascript;
	location / {
			proxy_pass http://localhost:8443;
	}
	listen 80.87.197.80:80;
}
server {
	server_name mia-chatbot.ru www.mia-chatbot.ru;
	ssl_certificate "/var/www/httpd-cert/www-root/mia-chatbot.ru_le2.crtca";
	ssl_certificate_key "/var/www/httpd-cert/www-root/mia-chatbot.ru_le2.key";
	ssl_ciphers EECDH:+AES256:-3DES:RSA+AES:!NULL:!RC4;
	ssl_prefer_server_ciphers on;
	ssl_protocols TLSv1 TLSv1.1 TLSv1.2 TLSv1.3;
	ssl_dhparam /etc/ssl/certs/dhparam4096.pem;
	charset off;
	index index.php index.html;
	disable_symlinks if_not_owner from=$root_path;
	include /etc/nginx/vhosts-includes/*.conf;
	include /etc/nginx/vhosts-resources/mia-chatbot.ru/*.conf;
	access_log /var/www/httpd-logs/mia-chatbot.ru.access.log;
	error_log /var/www/httpd-logs/mia-chatbot.ru.error.log notice;
	ssi on;
	set $root_path /var/www/www-root/data/www/mia-chatbot.ru;
	root $root_path;
	gzip on;
	gzip_comp_level 5;
	gzip_disable "msie6";
	gzip_types text/plain text/css application/json application/x-javascript text/xml application/xml application/xml+rss text/javascript application/javascript;
	location / {
			proxy_pass http://localhost:8443;
	}
	listen 80.87.197.80:443 ssl;
}
